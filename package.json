{
  "name": "vue-base-enterprise",
  "private": true,
  "version": "2.0.0",
  "type": "module",
  "scripts": {
    "commitlint": "commitlint --edit",
    "dev": "vite --mode dev",
    "dev:qa": "vite --mode qa",
    "dev:prod": "vite --mode prod",
    "build:qa": "cross-env NODE_ENV=staging npx eslint ./ && vue-tsc && vite build --mode qa",
    "build:prod": "cross-env NODE_ENV=production npx eslint ./ && vue-tsc && vite build --mode prod",
    "lint": "cross-env NODE_ENV=local npx eslint ./",
    "lint:prod": "cross-env NODE_ENV=production npx eslint ./",
    "preview": "vite preview",
    "prepare": "husky",
    "postinstall": "husky"
  },
  "dependencies": {
    "@quasar/extras": "^1.17.0",
    "@types/isotope-layout": "^3.0.15",
    "@unhead/vue": "^2.0.10",
    "@vueuse/core": "^13.3.0",
    "isotope-layout": "^3.0.6",
    "pinia": "^3.0.3",
    "pinia-plugin-persistedstate": "^4.3.0",
    "quasar": "^2.18.1",
    "vue": "^3.5.16",
    "vue-i18n": "^11.1.5",
    "vue-router": "^4.5.1"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.8.1",
    "@commitlint/config-conventional": "^19.8.1",
    "@eslint/js": "^9.29.0",
    "@quasar/vite-plugin": "^1.9.0",
    "@types/node": "^24.0.1",
    "@vitejs/plugin-vue": "^5.2.4",
    "@vue/tsconfig": "^0.7.0",
    "cross-env": "^7.0.3",
    "eslint": "^9.29.0",
    "eslint-plugin-vue": "^10.2.0",
    "globals": "^16.2.0",
    "husky": "^9.1.7",
    "prettier": "^3.5.3",
    "sass": "^1.89.2",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.34.0",
    "vite": "^6.3.5",
    "vue-tsc": "^2.2.10"
  }
}
