{
  "name": "purple",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "commitlint": "commitlint --edit",
    "dev": "vite",
    "dev:qa": "vite --mode qa",
    "dev:prod": "vite --mode prod",
    "build:qa": "cross-env NODE_ENV=production npx eslint ./ && vue-tsc && vite build --mode qa",
    "build:prod": "cross-env NODE_ENV=production npx eslint ./ && vue-tsc && vite build --mode prod",
    "lint": "cross-env NODE_ENV=local npx eslint ./",
    "lint:prod": "cross-env NODE_ENV=production npx eslint ./",
    "preview": "vite preview",
    "prepare": "husky"
  },
  "dependencies": {
    "@quasar/extras": "^1.16.12",
    "@types/isotope-layout": "^3.0.13",
    "@unhead/vue": "^1.9.16",
    "@vueuse/core": "^10.11.1",
    "isotope-layout": "^3.0.6",
    "pinia": "^2.2.1",
    "pinia-plugin-persistedstate": "^3.2.1",
    "quasar": "^2.16.8",
    "vue": "^3.4.37",
    "vue-i18n": "^9.13.1",
    "vue-router": "^4.4.3"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.4.0",
    "@commitlint/config-conventional": "^19.2.2",
    "@eslint/js": "^9.8.0",
    "@quasar/vite-plugin": "^1.7.0",
    "@types/node": "^22.1.0",
    "@vitejs/plugin-vue": "^5.1.2",
    "@vue/tsconfig": "^0.5.1",
    "cross-env": "^7.0.3",
    "eslint": "^9.8.0",
    "eslint-plugin-vue": "^9.27.0",
    "globals": "^15.9.0",
    "husky": "^9.1.4",
    "prettier": "^3.3.3",
    "sass": "^1.77.8",
    "typescript": "^5.5.4",
    "typescript-eslint": "^8.0.1",
    "vite": "^5.4.0",
    "vue-tsc": "^2.0.29"
  },
  "overrides": {
    "eslint": "^9.8.0"
  }
}
