{
  "name": "vue-base-enterprise",
  "private": true,
  "version": "2.0.0",
  "type": "module",
  "scripts": {
    "commitlint": "commitlint --edit",
    "dev": "vite --mode dev",
    "dev:qa": "vite --mode qa",
    "dev:prod": "vite --mode prod",
  "build:qa": "cross-env NODE_ENV=staging npx eslint ./src && vue-tsc && vite build --mode qa",
  "build:prod": "cross-env NODE_ENV=production npx eslint ./src && vue-tsc && vite build --mode prod",
  "lint": "cross-env NODE_ENV=local npx eslint ./src",
  "lint:prod": "cross-env NODE_ENV=production npx eslint ./src",
    "preview": "vite preview",
    "prepare": "husky",
    "postinstall": "husky"
  },
  "dependencies": {
    "@capacitor/android": "^7.4.3",
    "@capacitor/cli": "^7.4.3",
    "@capacitor/core": "^7.4.3",
    "@capacitor/geolocation": "^7.1.5",
    "@capacitor/ios": "^7.4.3",
    "@quasar/extras": "^1.17.0",
    "@types/isotope-layout": "^3.0.15",
    "@unhead/vue": "^2.0.17",
    "@vueuse/core": "^13.9.0",
    "isotope-layout": "^3.0.6",
    "pinia": "^3.0.3",
    "pinia-plugin-persistedstate": "^4.5.0",
    "quasar": "^2.18.5",
    "vue": "^3.5.21",
    "vue-i18n": "^11.1.12",
    "vue-router": "^4.5.1"
  },
  "devDependencies": {
