    "cross-env": "^10.0.0",
    "eslint": "^9.36.0",
    "eslint-plugin-vue": "^10.4.0",
    "globals": "^16.4.0",
    "husky": "^9.1.7",
    "prettier": "^3.6.2",
    "sass": "^1.93.0",
    "typescript": "^5.9.2",
    "typescript-eslint": "^8.44.0",
    "vite": "^7.1.6",
    "vue-tsc": "^3.0.7"
